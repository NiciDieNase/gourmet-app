							"                    <td>{{category}}</td>",
							"                    <td>{{allergens}}</td>",
							"                </tr>",
							"            {{/each}}",
							"        </table>",
							"    {{/each}}",
							"`;",
							"",
							"pm.visualizer.set(template, {",
